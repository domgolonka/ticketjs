<div class="section first">
    <div class="section dark-grey p-t-20  p-b-20 m-b-50">
        <div class="container">
            <div class="row">
                <div class="col-md-6">
                    <h2>Wiki</h2>
                </div>
            </div>
        </div>
        <div class="clearfix"></div>
    </div>
    <div class="container p-t-30">
        <div class="row">
            <% _.each(items, function(item) { %>
            <div class="col-md-8">
                <div class="grid simple vertical green">
                    <div class="grid-title no-border">
                        <h4 id="<%= item.id %>"><span class="semi-bold"><%= item.name %></span>
                            <small class="p-r-10"><%= item.description %></small>
                        </h4>
                        <div class="tools"><a href="javascript:;" class="collapse"></a> <a href="#grid-config"
                                                                                           data-toggle="modal"
                                                                                           class="config"></a> <a
                                    href="javascript:;" class="reload"></a> <a href="javascript:;" class="remove"></a>
                        </div>
                    </div>
                    <div class="grid-body no-border" style="overflow: hidden; display: block;">
                        <div class="row-fluid">
                            <div>
                                <ol class="bold">
                                    <% _.each(item.list, function(help) { %>
                                    <li><a href="/help/<%= help.name %>"><span
                                                    class="normal"><%= help.title %></span></a></li>
                                    <% }) %>
                                </ol>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
        <% }) %>
    </div>
    <%- partial("mixin/searchbottom") %>
</div>